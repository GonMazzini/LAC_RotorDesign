  begin aerodrag;
    begin aerodrag_element;
      mbdy_name	tower;
      aerodrag_sections	uniform 10;
      nsec	2;
      sec	0 0.6 8.3;	tower bottom
      sec	115.63 0.6 5.5;	tower top
