    dll inpvec	4 2 # pitch 2 [rad];
    dll inpvec	4 3 # pitch 3 [rad];	
; Check tower clearence
    dll inpvec	5 1 # bltip tow min d [m];
; Tower fore-aft velocity
	mbdy state	vel towertop 1 1 global only 2 # tower top fa vel;
; Floating 
